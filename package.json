{
  "name": "@bmd-studio/genstack-container-pg-jwt-auth",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index",
  "scripts": {
    "setup": "yarn install",
    "start": "node -r ts-node/register src/index.ts",
    "start:debug": "yarn start --inspect=0.0.0.0:${DEFAULT_DEBUG_PORT:-9229}",
    "start:prod": "node dist/index.js",
    "start:pm2": "pm2-runtime pm2.config.js",
    "start:docker": "yarn build && yarn dc:dev up",
    "start:docker-prod": "yarn dc up",
    "dc": "docker-compose --env-file=./env/defaults -f docker-compose.yml -f docker-compose.standalone.yml --compatibility",
    "dc:dev": "cross-env NODE_ENV=development GS_ENV=development yarn dc -f docker-compose.development.yml",
    "test": "yarn test:integration",
    "test:integration": "jest --testPathPattern=integration/",
    "test:container": "cross-env TEST_MODE=container yarn test:integration",
    "build": "tsc",
    "build:watch": "yarn build --watch",
    "build:docker": "yarn dc:dev build",
    "build:docker-prod": "yarn dc build",
    "clean": "rimraf dist/ && rimraf coverage/ && rimraf node_modules/"
  },
  "dependencies": {
    "apollo-server-express": "2.25.2",
    "bcrypt": "5.0.1",
    "chalk": "4.1.1",
    "debug": "4.3.2",
    "dotenv-parse-variables": "2.0.0",
    "express": "4.17.1",
    "graphql": "15.5.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "pg": "8.6.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/debug": "4.1.7",
    "@types/dotenv-parse-variables": "0.2.0",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "8.5.4",
    "@types/lodash": "4.14.171",
    "@types/pg": "7.14.11",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "cross-env": "7.0.3",
    "get-port": "5.1.1",
    "http-status-codes": "2.1.4",
    "jest": "26.6.3",
    "pm2": "4.5.6",
    "rimraf": "3.0.2",
    "supertest": "6.1.4",
    "testcontainers": "7.18.1",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.3.5"
  }
}
